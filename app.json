{
  "expo": {
    "name": "FitWalk",
    "slug": "FitWalk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "updates": {
      "enabled": false
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zigmund.FitWalk",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zigmund.FitWalk"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/logo.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "BASE_URL": "https://api.weatherapi.com/v1",
      "API_KEY": "70f402928e384a678f152737250705",
      "eas": {
        "projectId": "4b27a672-d3d5-4ca5-b016-7318801b1279"
      }
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-router",
        {
          "asyncRoutes": false
        }
      ]
    ]
  }
}
